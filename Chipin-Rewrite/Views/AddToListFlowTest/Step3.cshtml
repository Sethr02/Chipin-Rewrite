@model Chipin_Rewrite.Models.ViewModel.ListViewModel

<h2>Choose a Delivery Date</h2>
<form asp-action="ChooseDeliveryDate" method="post">
    <input type="hidden" name="listName" value="@Model.ListName" />
    <input type="date" name="deliveryDate" />
    <input type="submit" value="Create List" />
</form>