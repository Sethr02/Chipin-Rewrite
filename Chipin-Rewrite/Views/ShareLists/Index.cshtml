@model IEnumerable<Chipin_Rewrite.Models.Entities.ShareList>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ShareName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShareEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShareMessage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShareDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductListWalletTransaction)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ShareName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShareEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShareMessage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShareDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductListWalletTransaction.ProductListWalletTransactionId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ShareId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ShareId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ShareId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
