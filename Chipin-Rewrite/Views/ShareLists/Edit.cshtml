@model Chipin_Rewrite.Models.Entities.ShareList

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>ShareList</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ShareId" />
            <div class="form-group">
                <label asp-for="ShareName" class="control-label"></label>
                <input asp-for="ShareName" class="form-control" />
                <span asp-validation-for="ShareName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShareEmail" class="control-label"></label>
                <input asp-for="ShareEmail" class="form-control" />
                <span asp-validation-for="ShareEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShareMessage" class="control-label"></label>
                <input asp-for="ShareMessage" class="form-control" />
                <span asp-validation-for="ShareMessage" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShareDate" class="control-label"></label>
                <input asp-for="ShareDate" class="form-control" />
                <span asp-validation-for="ShareDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProductListWalletTransactionId" class="control-label"></label>
                <select asp-for="ProductListWalletTransactionId" class="form-control" asp-items="ViewBag.ProductListWalletTransactionId"></select>
                <span asp-validation-for="ProductListWalletTransactionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
